<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- <typeAlias> 쓸거면 여기에 -->
    <typeAliases>
            <typeAlias alias="MyMapDTO" type="com.yumpro.ddogo.mymap.domain.MyMapDTO"/>
    </typeAliases>
    <!-- 매퍼 파일 추가 -->
    <!-- 다수의 매퍼 파일을 포함한 경우 -->
    <mappers>
        <mapper resource="searchmap.xml"/>
        <mapper resource="classpath:mybatis/mymap/mymap.xml"/>
        <mapper resource="classpath:mybatis/mymap/userEmoReview.xml"/>

        <!-- 다른 매퍼 파일들도 추가할 수 있습니다. -->
    </mappers>

</configuration>