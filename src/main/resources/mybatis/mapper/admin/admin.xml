<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin">
    <select id="mapno" resultType="int">
        <![CDATA[
      select map_no
        from mymap
        where map_no=1
      ]]>
    </select>
</mapper>